          "_name": "counters1",
          "_customHTML": "<section class=\"counter1\" group=\"Counters\" plugins=\"ViewportChecker\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"4\" condition=\"!fullScreen\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"4\" condition=\"!fullScreen\">\n        <header>Show / Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Count\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Icons\" name=\"swIcon\" ckecked checked>\n        <select title=\"Cards Count\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"color\" title=\"Border color\" name=\"borderColor\" value=\"#4c4a92\">\n        <input type=\"color\" title=\"Block Color\" name=\"blockColor\" value=\"#25b8d0\">\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" title=\"Image\">\n            <input type=\"color\" value=\"#ffffff\" title=\"Color\" selected>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=9morbKMwCu4\" title=\"Video\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#13287d\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row justify-content-center align-center\">\n            <div class=\"col-md-3 col-sm-6 stats-item\" mbr-class=\"{'col-md-3': cardsAmount == '4','col-md-4': cardsAmount == '3','col-md-6': cardsAmount =='2', 'col-md-12': cardsAmount == '1'}\">\n                <div class=\"common-wrap d-flex flex-column\">\n                    <span mbr-if=\"swIcon\" mbr-icon class=\"mbr-iconfont mobi-mbri-timer mobi-mbri\"></span>\n                    <div class=\"fld-wrap\">\n                        <h3 class=\"mbr-fonts-style mbr-bold mbr-section-title count counter\" data-app-selector=\".stats-item .common-wrap .fld-wrap .counter\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-2\">300</h3>\n                        <h2 class=\"mbr-fonts-style mbr-card-title mbr-regular heading\" data-app-selector=\".stats-item .common-wrap .fld-wrap .mbr-card-title,.mbr-iconfont\" mbr-if=\"showTitle\" mbr-theme-style=\"display-4\">שעות עבודה</h2>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-md-3 col-sm-6 stats-item\" mbr-if=\"cardsAmount>1\" mbr-class=\"{'col-lg-3': cardsAmount == '4', 'col-md-4': cardsAmount == '3', 'col-md-6': cardsAmount =='2', 'col-md-12': cardsAmount == '1'}\">\n                <div class=\"common-wrap d-flex flex-column\">\n                    <span mbr-if=\"swIcon\" mbr-icon class=\"mbr-iconfont mobi-mbri-user-2 mobi-mbri\"></span>\n                    <div class=\"fld-wrap\">\n                        <h3 class=\"mbr-fonts-style mbr-bold mbr-section-title count counter\" data-app-selector=\".stats-item .common-wrap .fld-wrap .counter\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-2\">54</h3>\n                        <h2 class=\"mbr-fonts-style mbr-card-title mbr-regular headinge\" data-app-selector=\".stats-item .common-wrap .fld-wrap .mbr-card-title,.mbr-iconfont\" mbr-if=\"showTitle\" mbr-theme-style=\"display-4\">לקוחות מרוצים</h2>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-md-3 col-sm-6 stats-item\" mbr-if=\"cardsAmount>2\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n'col-md-4': cardsAmount == '3', 'col-md-6': cardsAmount =='2', 'col-md-12': cardsAmount == '1'}\">\n                <div class=\"common-wrap d-flex flex-column\">\n                    <span mbr-if=\"swIcon\" mbr-icon class=\"mbr-iconfont mbri-clock\"></span>\n                    <div class=\"fld-wrap\">\n                        <h3 class=\"mbr-fonts-style mbr-bold mbr-section-title count counter\" data-app-selector=\".stats-item .common-wrap .fld-wrap .counter\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-2\">500</h3>\n                        <h2 class=\"mbr-fonts-style mbr-card-title mbr-regular heading\" data-app-selector=\".stats-item .common-wrap .fld-wrap .mbr-card-title,.mbr-iconfont\" mbr-if=\"showTitle\" mbr-theme-style=\"display-4\">שעות עבודה</h2>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-md-3 col-sm-6 stats-item\" mbr-if=\"cardsAmount>3\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n'col-md-4': cardsAmount == '3', 'col-md-6': cardsAmount =='2', 'col-md-12': cardsAmount == '1'}\">\n                <div class=\"common-wrap d-flex flex-column\">\n                    <span mbr-if=\"swIcon\" mbr-icon class=\"mbr-iconfont mbri-magic-stick\"></span>\n                    <div class=\"fld-wrap\">\n       