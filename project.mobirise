          "_name": "form2",
          "_customHTML": "<section class=\"form2\" group=\"Forms & Maps\" plugins=\"formstyler, datepicker\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!fullScreen\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!fullScreen\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Contacts Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcons\" checked>\n        <input type=\"checkbox\" title=\"List Title\" name=\"showListTitle\" checked>\n        <input type=\"color\" name=\"listTitleHoverColor\" title=\"List Hover Color\" value=\"#25b8d0\" condition=\"showListTitle\">\n        <input type=\"checkbox\" title=\"List Text\" name=\"showListText\">\n        <input type=\"checkbox\" title=\"Social Icons\" name=\"showSocialIcons\">\n        <select title=\"List items\" name=\"listItemsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n        <select title=\"Social Icons\" name=\"socialIconsCount\" condition=\"showSocialIcons\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <input type=\"checkbox\" title=\"Form Title\" name=\"showFormTitle\" checked>\n        <input type=\"checkbox\" title=\"Form Text\" name=\"showFormText\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\">\n        <header>Style</header>\n        <input type=\"color\" name=\"socIconsColor\" title=\"Soc icons\" value=\"#13287d\" condition=\"showSocialIcons\">\n        <input type=\"color\" name=\"socIconsHover\" title=\"Soc icons hover\" value=\"#279e64\" condition=\"showSocialIcons\">\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\" checked>\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#4c4a92\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#22c1db\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#22c1db\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#22c1db\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay&&bg.type=='image')\"></div>\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row\">\n            <div class=\"col-lg-6 mb-5 text-content\">\n\n                <h3 class=\"mbr-subtitle mbr-fonts-style mbr-semibold\" data-app-selector=\".text-content .mbr-subtitle\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-4\"></h3>\n                <h2 class=\"mbr-section-title align-left mbr-bold mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-2\" data-app-selector=\".text-content .mbr-section-title\">תיצרו קשר</h2>\n                <p class=\"mbr-text mbr-black align-left pb-4 mbr-regular mbr-light mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".text-content .mbr-text\" mbr-if=\"showText\">Lorem\n                    ipsum\n                    dolor sit amet, consectetur adipiscing elit. Etiam dignissim quis mi eu tristique. Cras et\n                    massa erat. Quisque tempus, dolor sit amet ultrices mattis.</p>\n                <div class=\"list-block mt-5\">\n                    <div class=\"card-img pb-3\" mbr-if=\"showIcons\">\n                        <span mbr-icon class=\"mbri-mobile mbr-iconfont\"></span>\n                    </div>\n                    <h4 class=\"list-item-title mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showListTitle\" data-app-selector=\".text-content .list-block .list-item-title\"><a href=\"tel:+972546569436\" class=\"text-primary\">0546569436</a></h4>\n                    <p class=\"mbr-fonts-style mbr-text list-item-text\" mbr-theme-style=\"display-4\" data-app-selector=\".text-content .list-block .list-item-text\" mbr-if=\"showListText\">24/7 Support\n                        <br> Call Us\n                    </p>\n                </div>\n                <div class=\"list-block\" mbr-if=\"listItemsAmount > 1\">\n                    <div class=\"card-img pb-3\" mbr-if=\"showIcons\">\n                        <span mbr-icon class=\"mbri-pin mbr-iconfont\"></span>\n                    </div>\n                    <h4 class=\"list-item-title mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showListTitle\" data-app-selector=\".text-content .list-block .list-item-title\">נתניה</h4>\n                    <p class=\"mbr-fonts-style mbr-text list-item-text\" mbr-theme-style=\"display-4\" data-app-selector=\".text-content .list-block .list-item-text\" mbr-if=\"showListText\">Find Us\n                        Location</p>\n                </div>\n                <div class=\"list-block last\" mbr-if=\"listItemsAmount > 2\">\n                    <div class=\"card-img pb-3\" mbr-if=\"showIcons\">\n                        <span mbr-icon class=\"mbri-letter mbr-iconfont\"></span>\n                    </div>\n                    <h4 class=\"list-item-title mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showListTitle\" data-app-selector=\".text-content .list-block .list-item-title\">\n                        <a href=\"mailto:aviclean213@gmail.com\" class=\"text-primary\">aviclean213@gmail.com</a></h4>\n                    <p class=\"mbr-fonts-style mbr-text list-item-text\" mbr-theme-style=\"display-4\" data-app-selector=\".text-content .list-block .list-item-text\" mbr-if=\"showListText\">\n                        Email</p>\n                </div>\n                <div class=\"social mt-4\" mbr-if=\"showSocialIcons\">\n                    <span mbr-icon class=\"mbr-iconfont fa-facebook-square fa\"></span>\n                    <span mbr-icon class=\"mbr-iconfont fa-twitter-square fa\" mbr-if=\"socialIconsCount > 1\"></span>\n                    <span mbr-icon class=\"mbr-iconfont fa-linkedin-square fa\" mbr-if=\"socialIconsCount > 2\"></span>\n                    <span mbr-icon class=\"mbr-iconfont fa-instagram fa\" mbr-if=\"socialIconsCount > 3\"></span>\n             